import re
Test= '^[a-z0-9]+[\._]?[a-z0-9]+[@]\w+[.]\w{2,3}$'
def check(email):
    if (re.search(Test, email)):
        print("Valid Email")
    else:
        print("Invalid Email")
if __name__ == '__main__':
    email = "vaibhavsolunke110@gmail.com"
    check(email)

    email = "vaibhav@#gmail.com"
    check(email)


